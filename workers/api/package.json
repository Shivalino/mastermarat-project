{
    "name":  "mastermarat-api",
    "version":  "1.0.0",
    "description":  "MasterMarat API Worker for protected video delivery",
    "main":  "src/worker.js",
    "scripts":  {
                    "dev":  "wrangler dev",
                    "deploy":  "wrangler deploy --env production",
                    "deploy:staging":  "wrangler deploy --env staging",
                    "preview":  "wrangler dev --remote",
                    "test":  "npm run test:api",
                    "test:api":  "node ../../scripts/test-api.js",
                    "login":  "wrangler login",
                    "whoami":  "wrangler whoami",
                    "logs":  "wrangler tail",
                    "logs:prod":  "wrangler tail --env production",
                    "lint":  "eslint src/",
                    "lint:fix":  "eslint src/ --fix",
                    "format":  "prettier --write src/**/*.js",
                    "format:check":  "prettier --check src/**/*.js"
                },
    "keywords":  [
                     "cloudflare",
                     "workers",
                     "edtech",
                     "api",
                     "video-streaming",
                     "mastermarat",
                     "r2-storage",
                     "osteopathy",
                     "health-education"
                 ],
    "author":  "MasterMarat Team",
    "license":  "MIT",
    "repository":  {
                       "type":  "git",
                       "url":  "https://github.com/Shivalino/mastermarat-project.git"
                   },
    "homepage":  "https://mastermarat.com",
    "devDependencies":  {
                            "eslint":  "^8.57.1",
                            "prettier":  "^3.6.2",
                            "wrangler":  "^4.22.0"
                        },
    "engines":  {
                    "node":  "\u003e=18.0.0",
                    "npm":  "\u003e=8.0.0"
                }
}
